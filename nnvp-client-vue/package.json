  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "oxlint": "^1.24.0",
    "vite": "^6.4.1"
  }
